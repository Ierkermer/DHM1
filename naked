local notifSound = Instance.new("Sound",workspace)
	notifSound.PlaybackSpeed = 1.5
	notifSound.Volume = 0.16
	notifSound.SoundId = "rbxassetid://170765130"
	notifSound.PlayOnRemove = true
	notifSound:Destroy()
	game.StarterGui:SetCore("SendNotification", {Title = "Naked", Text = ":D", Icon = "rbxassetid://505845268", Duration = 3, Button1 = "impostor"})
	while wait() do
		pcall(function()
			if game.Players.LocalPlayer.Character:FindFirstChild("Shirt") then
				game.Players.LocalPlayer.Character.Shirt:Destroy()
			elseif game.Players.LocalPlayer.Character:FindFirstChild("Pants") then
				game.Players.LocalPlayer.Character.Pants:Destroy()
			end
		end)
	end
end)
